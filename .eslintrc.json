{
    "env": {
        "browser": true
    },
		"extends": "eslint:recommended",
		"parser": "babel-eslint",
    "parserOptions": {
				"sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            "tab"
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}