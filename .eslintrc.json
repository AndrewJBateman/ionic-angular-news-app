{
    "env": {
        "browser": true
    },
		"extends": "eslint:recommended",
		"parser": "babel-eslint",
    "parserOptions": {
				"ecmaVersion": 2018,
				"sourceType": "module"
    },
    "rules": {
        "indent": 0,
        "linebreak-style": 0,
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
				]
    }
}